public class BackwardString {
	
	public static void back(String s) {
		int l=s.length();
		char [] arr= new char [l];
		for(int i=0; i<l; i++) {
			char c=s.charAt(i);
			arr[i]=c;
		}
		for(int i=l-1; i>=0; i--) {
			System.out.println(arr[i]);
		}
		
		
	}
	public static void main (String[] args) {
		String a="apple";
		back(a);
	}

}
