public class wordCounter {
	public static void words(String s) {
		int l=s.length();
		int count=1;
		char [] arr= new char [l];
		for(int i=0; i<l; i++) {
			char c=s.charAt(i);
			if(c==' ') count++;
		}
		System.out.println(count);
	
	}
	public static void main (String[] args) {
		String a="Four score and seven years ago";
		words(a);
	}


}
