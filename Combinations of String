import java.util.*;

public class Comb {
	public static void combination(String[] a, int n) {
		int l=a.length;
		Random r= new Random();
		String [] b=new String [l];
		for(int i=0;i<n;i++) {
			int rand=r.nextInt(l)+1;
			b[i]=a[rand];
		}
		for(int i=0;i<n;i++) {
		 System.out.println(b[i]);
		}
		
		
	}
	
	
	
	public static void main(String[] args) {
		String [] arr= {"Amy", "Bob", "Carl", "David", "Ethan", "Frank", "Gail"};
		int number=3;
		combination(arr,number);
		
	}
	

}
